{
    "name": "ringierimu/multi-tenant",
    "description": "Laravel Multi-Tenancy package",
    "keywords": ["multi-tenancy", "tenant", "laravel", "multi-domain", "tenancy", "multi-tenant"],
    "type": "library",
    "version": "0.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Norby Baruani",
            "email": "norby.baruani@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.6 || ^5.7 || ^5.8"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Ringierimu\\MultiTenant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ringierimu\\MultiTenant\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ringierimu\\MultiTenant\\MultiTenancyServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^7.0",
        "orchestra/testbench": "^3.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
