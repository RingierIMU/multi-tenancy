{
    "name": "ringierimu/multi-tenancy",
    "description": "Laravel Multi-Tenancy package",
    "keywords": ["multi-tenancy", "tenant", "laravel", "multi-domain", "tenancy", "multi-tenant"],
    "type": "library",
    "version": "0.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Norby Baruani",
            "email": "norby.baruani@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.6 || ^5.7 || ^5.8"
    },
    "autoload": {
        "psr-4": {
            "Ringierimu\\MultiTenancy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ringierimu\\MultiTenancy\\MultiTenancyServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
